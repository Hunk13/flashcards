<h1>All users</h1>

<table class="table table-striped">
    <tr>
      <th>Имя пользователя</th>
      <th>User ID</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <th>
        <%= link_to user.name, user %>
      </th>
      <th>
        <%= user.id %>
      </th>
    </tr>
  <% end %>
</table>